ny field.\n        // A regular user can ONLY update the 'status' and 'notes' fields of a shift assigned to them.\n        allow update: if isPrivileged() || (request.auth.uid == resource.data.userId && request.resource.data.diff(resource.data).affectedKeys().hasOnly(['status', 'notes', 'confirmedAt']));\n    }\n\n    // PROJECTS collection & files subcollection\n    match /projects/{projectId} {\n        // Any authenticated user can view projects.\n        allow read: if request.auth != null;\n        \n        // Only admins/owners can create, update, or delete projects.\n        allow write: if isPrivileged();\n\n        // FILES subcollection\n        match /files/{fileId} {\n            // Any authenticated user can view files.\n            allow read: if request.auth != null;\n\n            // Any authenticated user can upload files.\n            allow create: if request.auth != null;\n            \n            // Users can delete their own files, admins/owners can delete any file.\n            allow delete: if request.auth.uid == resource.data.uploaderId || isPrivileged();\n        }\n    }\n    \n    // ANNOUNCEMENTS collection\n    match /announcements/{announcementId} {\n        // Any authenticated user can read announcements.\n        allow read: if request.auth != null;\n        \n        // Only admins/owners can create/update/delete announcements.\n        allow write: if isPrivileged();\n        \n        match /acknowledgedBy/{userId} {\n             // A user can only acknowledge for themselves.\n             allow write: if request.auth.uid == userId;\n             // Only privileged users can see who has acknowledged.\n             allow read: if isPrivileged();\n        }\n    }\n    \n    // SETTINGS collection\n    match /settings/{settingId} {\n        // Only the owner can read or write to the settings collection.\n        allow read, write: if isOwner();\n    }\n  }\n}\n"}]}}
